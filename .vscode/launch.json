  "version": "0.2.0",
  "configurations": [
  
    {
      "name": "Django: makemigrations",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "console": "internalConsole",
      "args": [
        "makemigrations",
      ],
      "django": true
    },
    {
      "name": "Django: migrate",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "console": "internalConsole",
      "args": [
        "migrate",
      ],
      "django": true
    },
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "console": "internalConsole",
      "args": [
        "runserver",
      ],
      "django": true
    }
  ]
}